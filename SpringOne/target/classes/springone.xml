<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="com.scp.java.eleven.SpringOne.Laptop" id="laptopref" > 
			<property name="laptopId" value="10"></property>
			<property name="laptopName" value="thikpad"></property>
			<property name="vendor" ref="venRef"></property>
			<property name="price" value="58000.0"></property>
	</bean> 

	<bean class="com.scp.java.eleven.SpringOne.Vendor" id="venRef"> 
		<property name="vid" value="12"></property>
		<property name="vname" value="shwetal"></property>
	</bean>   
</beans>